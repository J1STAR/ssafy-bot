{
  "manifest_version": 2,
  "name": "SSAFY ChatBot Extension",
  "version": "1.0",
  "description": "ChatBot Extension for SSAFY User",
  "icons": { "16": "ssafy-icon.png", "48": "ssafy-icon.png", "128": "ssafy-icon.png" },
  "permissions": ["*://edu.ssafy.com/*"],
  "content_scripts": [{
    "matches": ["*://edu.ssafy.com/*"],
    "css": ["css/app.75da7512.css"],
    "js": ["background.js"],
    "run_at":"document_idle"
  }],
  "web_accessible_resources": ["index.html", "css/app.75da7512.css", "js/app.dc5df974.js", "js/chunk-vendors.bd33d734.js"]
}